{
    "plugins": [ "plugins/markdown" ],
    "recurseDepth": 10,
    "source": {
        "include": [ "pipo", "README.md" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "logoFile": "logo-white.svg",
        "systemName": "PIPO",
        "theme": "sandstone",
        "linenums": true,
        "syntaxTheme": "dark",
        "navType": "inline",
        "default": {
            "staticFiles": {
                "include": [ "./data/logo.svg", "./data/logo-white.svg" ]
            }
        }
    },
    "opts": {
        "template": "node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}
