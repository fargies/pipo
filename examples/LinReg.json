{
  "pipe": "Aggregate|MathEval#linReg|MathEval#var|MathEval#values|Remove",

  "MathEvalConfig#linReg": {
    "expr": "linReg(map(items, f(i) = [ (i.date - items[count(items)].date) / 86400, i.close ]))",
    "property": "linReg"
  },
  "MathEvalConfig#var": {
    "expr": "map(items, f(i) = (i.close - (((i.date - items[count(items)].date) / 86400) * linReg[1] + linReg[2])) / i.close * 100)",
    "property": "var"
  },
  "MathEvalConfig#values": {
    "expr": "map(items, f(i) = i.close)",
    "property": "values"
  },
  "RemoveConfig": {
    "property": "items"
  }
}
