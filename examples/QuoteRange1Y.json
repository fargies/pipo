{
  "pipe": "SubPipe#from|StdOut|Aggregate|MathEval#min|Date#from|Add#to|Add#symbol|NLDate#to",

  "SubPipeConfig#from": {
    "pipe": [
      {
        "pipe": "WaitFor|Duplicate|Rename|Replace|PouchDBIn|MathEval#nextDay",
        "WaitForConfig": {
          "property": "symbol"
        },
        "DuplicateConfig": {
          "property": "symbol",
          "newName": "PouchDBInConfig.database"
        },
        "RenameConfig": {
          "property": "symbol",
          "newName": "AddConfig#symbol.value"
        },
        "ReplaceConfig": {
          "property": "PouchDBInConfig.database",
          "pattern": "^",
          "newSubstr": "/var/db/quotes/"
        },
        "MathEvalConfig#nextDay": {
          "expr": "date + (24*60*60)",
          "property": "from"
        },
        "selector": {},
        "createIndex": { "fields": [ "date" ], "name": "dateIndex" },
        "sort": [ { "date": "desc" } ],
        "limit": 1
      },
      {
        "pipe": "NLDate#from",
        "NLDateConfig#from": {
          "property": "from",
          "format": "X"
        },
        "from": "one year ago"
      }
    ]
  },

  "AggregateConfig": {
    "property": "from"
  },
  "MathEvalConfig#min": {
    "expr": "min(from)",
    "property": "from"
  },
  "DateConfig#from": {
    "property": "from",
    "inFormat": "X",
    "outFormat": "YYYY-MM-DD"
  },


  "AddConfig#to": {
    "property": "to",
    "value": "now"
  },
  "AddConfig#symbol": {
    "property": "symbol"
  },
  "NLDateConfig#to": {
    "property": "to",
    "format": "YYYY-MM-DD"
  }
}
